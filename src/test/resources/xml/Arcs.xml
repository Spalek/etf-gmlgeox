<Test xmlns="http://www.example.org/test/arc" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xmlns:gml="http://www.opengis.net/gml/3.2">
    <geom>
        <gml:Curve gml:id="ARC1" srsName="urn:adv:crs:ETRS89_UTM32">
            <gml:segments>
                <gml:Arc numArc="1">
                    <gml:posList>1 0 0 1 -1 0</gml:posList>
                </gml:Arc>
            </gml:segments>
        </gml:Curve>
    </geom>
    <geom>
        <gml:Curve gml:id="ARC2" srsName="urn:adv:crs:ETRS89_UTM32">
            <gml:segments>
                <gml:Arc numArc="1">
                    <gml:posList>-1 0 0 1 1 0</gml:posList>
                </gml:Arc>
            </gml:segments>
        </gml:Curve>
    </geom>
    <geom>
        <gml:Curve gml:id="LS" srsName="urn:adv:crs:ETRS89_UTM32">
            <gml:segments>
                <gml:LineStringSegment>
                    <gml:posList>-1 1 1 1</gml:posList>
                </gml:LineStringSegment>
            </gml:segments>
        </gml:Curve>
    </geom>
</Test>
